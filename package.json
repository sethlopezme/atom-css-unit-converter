{
  "name": "atom-css-unit-converter",
  "main": "./lib/main",
  "version": "0.1.1",
  "description": "Performs conversions between different CSS units.",
  "author": {
    "name": "Seth Lopez",
    "url": "http://sethlopez.me"
  },
  "keywords": [
    "css",
    "sass",
    "scss",
    "less",
    "unit",
    "convert",
    "converter",
    "conversion",
    "px",
    "em",
    "rem"
  ],
  "repository": "https://github.com/sethlopezme/atom-css-unit-converter",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.1.0",
    "space-pen": "^5.1.1"
  },
  "activationCommands": {
    "atom-text-editor": [
      "atom-css-unit-converter:px-to-em",
      "atom-css-unit-converter:px-to-rem",
      "atom-css-unit-converter:em-to-px",
      "atom-css-unit-converter:em-to-rem",
      "atom-css-unit-converter:rem-to-px",
      "atom-css-unit-converter:rem-to-em",
      "atom-css-unit-converter:interactive"
    ]
  }
}
